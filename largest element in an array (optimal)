#include<bits/stdc++.h>
using namespace std;


int LargestElement(int arr[],int sz){

    int largest=arr[0];
    for(int i=0;i<sz;i++){
        if(arr[i]>largest){
            largest=arr[i];
        }
    }
    return largest;
}


int main(){
    int arr[]={1,3,5,4,2};
    int sz=sizeof(arr)/sizeof(int);

    cout<<"largest element:"<<LargestElement(arr,sz)<<endl;
    return 0;
}
